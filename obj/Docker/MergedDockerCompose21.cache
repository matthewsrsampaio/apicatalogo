name: dockercompose13484724540897623862
services:
  apicatalogo:
    build:
      context: D:\DEV\Projetos C#\apicatalogo
      dockerfile: APICatalogo/Dockerfile
      args:
        BUILD_CONFIGURATION: Debug
        LAUNCHING_FROM_VS: "true"
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: APICatalogo
      target: base
    container_name: apicatalogo.api
    entrypoint:
      - dotnet
      - --roll-forward
      - Major
      - /VSTools/DistrolessHelper/DistrolessHelper.dll
      - --wait
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "5000"
      ASPNETCORE_HTTPS_PORTS: "5001"
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: apicatalogo:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages  "/app/bin/Debug/net8.0/APICatalogo.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: dotnet --roll-forward Major /VSTools/DistrolessHelper/DistrolessHelper.dll --stop dotnet
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5000
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 5001
        published: "5001"
        protocol: tcp
    tty: true
    volumes:
      - type: bind
        source: C:\Users\Matthews\AppData\Roaming\Microsoft\UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Matthews\AppData\Roaming\ASP.NET\Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: D:\DEV\Projetos C#\apicatalogo\APICatalogo
        target: /app
        bind:
          create_host_path: true
      - type: bind
        source: D:\DEV\Projetos C#\apicatalogo
        target: /src
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Matthews\vsdbg\vs2017u5
        target: /remote_debugger
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Matthews\.nuget\packages
        target: /.nuget/packages
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Matthews\AppData\Roaming\ASP.NET\Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Matthews\AppData\Roaming\Microsoft\UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Program Files\Microsoft Visual Studio\2022\Community\MSBuild\Sdks\Microsoft.Docker.Sdk\tools\linux-x64\net8.0
        target: /VSTools
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Program Files\Microsoft Visual Studio\2022\Community\Common7\IDE\CommonExtensions\Microsoft\HotReload
        target: /HotReloadAgent
        read_only: true
        bind:
          create_host_path: true
  apicatalogo.database:
    container_name: apicatalogo.database
    environment:
      POSTGRES_DB: apicatalogo
      POSTGRES_PASSWORD: "123456"
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    volumes:
      - type: bind
        source: D:\DEV\Projetos C#\apicatalogo\postgres-data
        target: /var/lib/postgresql/data
        bind:
          create_host_path: true
  common_db:
    environment:
      POSTGRES_MULTIPLE_DATABASES: db1
      POSTGRES_PASSWORD: "123456"
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      app-network: null
    ports:
      - mode: ingress
        target: 5432
        published: "5436"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: D:\DEV\Projetos C#\apicatalogo\postgres-data
        target: /var/lib/postgresql/data
        bind:
          create_host_path: true
networks:
  app-network:
    name: dockercompose13484724540897623862_app-network
    driver: bridge`
  default:
